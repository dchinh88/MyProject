@model IEnumerable<ProjectNet.Models.Binhluan>

@{
    ViewData["Title"] = "Index";
}

<h1 style="margin-top: 100px">Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.IDKH)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IDSP)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NOIDUNGBL)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.THOIGIANBL)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.IDKH)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IDSP)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NOIDUNGBL)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.THOIGIANBL)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.ID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.ID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.ID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
